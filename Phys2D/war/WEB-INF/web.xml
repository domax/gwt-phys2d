<?xml version="1.0" encoding="UTF-8"?>
<web-app
	version="2.5"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xmlns="http://java.sun.com/xml/ns/javaee">

	<servlet>
		<servlet-name>communicatorServlet</servlet-name>
		<servlet-class>com.dominichenko.pet.gwt.phys2d.demo.server.CommunicatorImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>communicatorServlet</servlet-name>
		<url-pattern>/demophys2d/communicator</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
